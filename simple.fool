let
	fun getTemperature:int(celsius:bool)
		if(celsius)
		then {20}
		else {20+243};
		
	fun getPressure:int(pascal:bool)
		if(pascal)
		then {10}
		else {3};
	
	fun main:int(std:bool, property:(bool)->int)
		if(std)
		then {property(std)}
		else {property(!std)};
in
	print(main(true, getTemperature));