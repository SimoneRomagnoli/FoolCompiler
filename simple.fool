let
	fun getTemperature:int(celsius:bool)
		if(celsius)
		then {20}
		else {20+243};
		
	fun isHot:bool(degrees:int)
		if(degrees>=25)
		then {true}
		else {false};
	
	fun main:int(std:bool, temp:(bool)->int, hot:(int)->bool)
		if(std)
		then {temp(std)}
		else {hot(26)};
in
	print(main(true, getTemperature, isHot));